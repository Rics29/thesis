const { test, expect } = require('@playwright/test') ;
  test('basic test', async ({ page }) => {
    test.setTimeout(12000000);
    await page.goto('http://172.27.22.216:8001/login');
    // Go to http://172.27.22.216:8001/login#login
    await page.goto('http://172.27.22.216:8001/login#login');
    // Click text=Email Password Show Forgot Password? >> [placeholder="jane\@example\.com"]
    await page.locator('text=Email Password Show Forgot Password? >> [placeholder="jane\\@example\\.com"]').click();
    // Fill text=Email Password Show Forgot Password? >> [placeholder="jane\@example\.com"]
    await page.locator('text=Email Password Show Forgot Password? >> [placeholder="jane\\@example\\.com"]').fill('Administrator');
    // Press Tab
    await page.locator('text=Email Password Show Forgot Password? >> [placeholder="jane\\@example\\.com"]').press('Tab');
    // Fill [placeholder="•••••"]
    await page.locator('[placeholder="•••••"]').fill('admin');
    // Press Enter
    await Promise.all([
      page.waitForNavigation(/*{ url: 'http://172.27.22.216:8001/app' }*/),
      page.locator('[placeholder="•••••"]').press('Enter')
    ]);
    await page.goto('http://172.27.22.216:8001/app/home');
  // Click text=HR
  await page.locator('text=HR').click();
  await expect(page).toHaveURL('http://172.27.22.216:8001/app/hr');
  // Click text=Employee Health Insurance
  await page.locator('text=Employee Health Insurance').click();
  await expect(page).toHaveURL('http://172.27.22.216:8001/app/employee-health-insurance');
  // Click button:has-text("Add Employee Health Insurance")
  await page.locator('button:has-text("Add Employee Health Insurance")').click();
  // Fill text=Health Insurance Name undefined >> input[type="text"]
  await page.locator('text=Health Insurance Name undefined >> input[type="text"]').fill('OLD age');
  // Click button:has-text("Save")
  await page.locator('button:has-text("Save")').click();
  // Click button:has-text("Add Employee Health Insurance")
  await page.locator('button:has-text("Add Employee Health Insurance")').click();
  // Fill text=Health Insurance Name undefined >> input[type="text"]
  await page.locator('text=Health Insurance Name undefined >> input[type="text"]').fill('grownup');
  // Click text=Save >> nth=3
  await page.locator('text=Save').nth(3).click();
  // Click text=Employee Health Insurance List View Report Dashboard Kanban Refresh User Permiss
  await page.locator('text=Employee Health Insurance List View Report Dashboard Kanban Refresh User Permiss').click();
  // Click button:has-text("Add Employee Health Insurance")
  await page.locator('button:has-text("Add Employee Health Insurance")').click();
  // Fill text=Health Insurance Name undefined >> input[type="text"]
  await page.locator('text=Health Insurance Name undefined >> input[type="text"]').fill('foliop');
  // Click text=Save >> nth=4
  await page.locator('text=Save').nth(4).click();
  // Click button:has-text("Add Employee Health Insurance")
  
  await page.locator('text=Employee Health Insurance List View Report Dashboard Kanban Refresh User Permiss').click();
  // Click button:has-text("Add Employee Health Insurance")
  await page.locator('button:has-text("Add Employee Health Insurance")').click();
  // Fill text=Health Insurance Name undefined >> input[type="text"]
  await page.locator('text=Health Insurance Name undefined >> input[type="text"]').fill('iopon');
  // Click text=Save >> nth=4
  await page.locator('text=Save').nth(5).click();
  
  
  /*after this the next data wont be saved and type for previous data to be stored*/


  await page.locator('text=Employee Health Insurance List View Report Dashboard Kanban Refresh User Permiss').click();
  // Click button:has-text("Add Employee Health Insurance")
  await page.locator('button:has-text("Add Employee Health Insurance")').click();
  // Fill text=Health Insurance Name undefined >> input[type="text"]
  await page.locator('text=Health Insurance Name undefined >> input[type="text"]').fill('onon');
  // Click text=Save >> nth=4
  await page.locator('text=Save').nth(6).click();

});
